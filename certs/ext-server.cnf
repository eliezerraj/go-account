[req]
default_md = sha256
prompt = no
req_extensions = v3_ext
distinguished_name = req_distinguished_name

[req_distinguished_name]
CN = go-account.architecture.caradhras.io

[v3_ext]
keyUsage = critical,digitalSignature,keyEncipherment
extendedKeyUsage = critical,serverAuth,clientAuth
subjectAltName = @alt_names

[alt_names]
DNS.1 = localhost
DNS.2 = go-account.architecture.caradhras.io
DNS.3 = go-account-local.architecture.caradhras.io
IP.1 = 0.0.0.0
IP.1 = 127.0.0.1